{
  "name": "show-cased",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "heroku-prebuild": "cd client && npm i",
    "dev-client": "cd client && npm start",
    "build": "cd client && npm run build && npm run build-storybook",
    "storybook": "cd client && npm run storybook",
    "dev": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client\"",
    "dev-story": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client\" \"npm run storybook\"",
    "dev-server": "nodemon server.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Caruki/show-cased.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Caruki/show-cased/issues"
  },
  "homepage": "https://github.com/Caruki/show-cased#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  },
  "cacheDirectories": [
    "client/node_modules",
    "node_modules"
  ],
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "babel-eslint": "^10.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.0",
    "mongoose": "^5.9.27",
    "node-fetch": "^3.1.1"
  }
}
